<html>
    <head>
        <%- include('../partials/head'); %>
    </head>

<body>

    <header>
        <%- include('../partials/header'); %>
    </header>

    <h1>Cities</h1>

    <ul>
        <% cities.forEach(function(city) { %>
          <li>
            <strong><%= city.nameEl %></strong>
            <%= city.nameEn %>
            <%= city.lat %>, <%= city.lng %>
          </li>
        <% }); %>
    </ul>


    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>